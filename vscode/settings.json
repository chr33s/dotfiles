{
  "better-comments.highlightPlainText": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "fixme",
      "color": "#CC0000",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "note",
      "color": "#CC00CC",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "breadcrumbs.enabled": false,
  "debug.showInStatusBar": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
    // "source.organizeImports": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.experimental.stickyScroll.enabled": true,
  "editor.formatOnSave": true,
  "editor.fontFamily": "FiraCode-Retina, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.fontWeight": "400",
  "editor.glyphMargin": true,
  "editor.guides.bracketPairs": "active",
  "editor.lineHeight": 22,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": false,
  "eslint.alwaysShowStatus": true,
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "explorer.confirmDelete": false,
  "explorer.experimental.fileNesting.enabled": true,
  "explorer.experimental.fileNesting.expand": false,
  "explorer.experimental.fileNesting.patterns": {
    ".env": "*.env, .env.*, env.d.ts",
    ".gitignore": ".gitattributes, .gitmodules, .gitmessage, .mailmap, .git-blame*",
    "*.js": "$(capture).js.map, $(capture).min.js, $(capture).css",
    "*.jsx": "$(capture).js, , $(capture).css",
    "*.ts": "$(capture).js, $(capture).*.ts, $(capture).d.ts, $(capture).css",
    "*.tsx": "$(capture).ts, $(capture).*.tsx, $(capture).d.ts, $(capture).css",
    "app.json": ".buckconfig, metro.config.js",
    "dockerfile": ".dockerignore, dockerfile*",
    "firebase.json": ".firebase*, firebase.*.json, firebase.*.rules, firebase*.log, firestore*, pubsub*, ui*, credentials.json, service-account.*",
    "index.d.ts": "*.d.ts",
    "go.mod": ".air*, go.sum",
    "gemfile": ".ruby-version, gemfile.lock",
    "package.json": ".babelrc*, babel.config.*, .browserslist*, .circleci*, .codecov, .commitlint*, .cssnanorc*, .editorconfig, .eslint*, .flowconfig, .github*, .gitlab*, .gitpod*, .huskyrc*, .htmlnanorc*, .jslint*, .lintstagedrc*, .markdownlint*, .mocha*, .node-version, .nodemon*, .npm*, .nvmrc, .pm2*, .pnp.*, .pnpm*, .prettier*, .postcssrc*, .releaserc*, .sentry*, .stackblitz*, .styleci*, .stylelint*, .tazerc*, .textlint*, .tool-versions, .travis*, .vscode*, .watchman*, .xo-config*, .yamllint*, .yarnrc*, api-extractor.json, appveyor*, ava.config.*, azure-pipelines*, bower.json, build.config.*, commitlint*, crowdin*, cypress.json, dangerfile*, dprint.json, grunt*, gulp*, jasmine.*, jenkins*, jest.*.js, jest.*.cjs, jsconfig.*, karma*, lerna*, lint-staged*, nest-cli.*, netlify*, next.config.*, nodemon*, nx.*, package-lock.json, playwright.config.*, pm2.*, pnpm*, postcss.config.*, prettier*, pullapprove*, puppeteer.config.*, renovate*, rollup.config.*, sanity.json, stylelint*, tailwind.config.*, tsconfig.*, tsdoc.*, tslint*, tsup.config.*, turbo*, typedoc*, vercel*, vetur.config.*, vite.config.*, vitest.config.*, webpack.config.*, workspace.json, yarn*",
    "podfile": "podfile.lock",
    "jest.config.*": "jest.*.js, jest-playwright.config.*"
  },
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.associations": {
    ".firebaserc": "json"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "prettier.requireConfig": true,
  "scm.defaultViewMode": "tree",
  "search.exclude": {
    "**/.cache": true,
    "**/.git": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true
  },
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontFamily": "FiraCodeNerdFontComplete-Retina, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.tabs.enabled": false,
  "window.autoDetectColorScheme": true,
  "window.nativeFullScreen": true,
  "window.nativeTabs": true,
  "window.restoreWindows": "all",
  "window.title": "${rootName}",
  "workbench.colorTheme": "GitHub Light",
  "workbench.fontAliasing": "antialiased",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "GitHub Dark",
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.startupEditor": "newUntitledFile",
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.stylelint": false
    }
  }
}